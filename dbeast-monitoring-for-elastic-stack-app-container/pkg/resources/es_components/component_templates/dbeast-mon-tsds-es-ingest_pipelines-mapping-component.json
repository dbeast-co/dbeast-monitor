{
  "template": {
    "settings": {},
    "mappings": {
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "elasticsearch": {
          "properties": {
            "cluster": {
              "properties": {
                "id": {
                  "type": "keyword",
                  "time_series_dimension": true
                }
              }
            },
            "node": {
              "properties": {
                "stats": {
                  "properties": {
                    "ingest_pipeline": {
                      "properties": {
                        "current": {
                          "type": "long",
                          "time_series_metric": "gauge"
                        },
                        "time_in_millis": {
                          "type": "long",
                          "time_series_metric": "gauge"
                        },
                        "count": {
                          "type": "long",
                          "time_series_metric": "gauge"
                        },
                        "in_rate": {
                          "type": "long",
                          "time_series_metric": "gauge"
                        },
                        "processors": {
                          "type": "object",
                          "properties": {
                            "number_in_pipeline": {
                              "type": "long",
                              "time_series_metric": "gauge"
                            },
                            "stats": {
                              "properties": {
                                "current": {
                                  "type": "long",
                                  "time_series_metric": "gauge"
                                },
                                "time_in_millis": {
                                  "type": "long",
                                  "time_series_metric": "gauge"
                                },
                                "count": {
                                  "type": "long",
                                  "time_series_metric": "gauge"
                                },
                                "failed": {
                                  "type": "long",
                                  "time_series_metric": "gauge"
                                }
                              }
                            },
                            "id": {
                              "type": "keyword",
                              "ignore_above": 256
                            },
                            "type": {
                              "type": "keyword",
                              "ignore_above": 256
                            }
                          }
                        },
                        "failed": {
                          "type": "long",
                          "time_series_metric": "gauge"
                        },
                        "id": {
                          "type": "keyword",
                          "time_series_dimension": true
                        }
                      }
                    },
                    "ingest": {
                      "properties": {
                        "enrich": {
                          "properties": {
                            "cache_stats": {
                              "properties": {
                                "hits": {
                                  "type": "long",
                                  "time_series_metric": "gauge"
                                },
                                "misses": {
                                  "type": "long",
                                  "time_series_metric": "gauge"
                                },
                                "hits_rate": {
                                  "type": "long",
                                  "time_series_metric": "gauge"
                                },
                                "evictions": {
                                  "type": "long",
                                  "time_series_metric": "gauge"
                                },
                                "count": {
                                  "type": "long",
                                  "time_series_metric": "gauge"
                                },
                                "hits_last_period": {
                                  "type": "long",
                                  "time_series_metric": "gauge"
                                }
                              }
                            },
                            "coordinator_stats": {
                              "properties": {
                                "executed_searches_total": {
                                  "type": "long",
                                  "time_series_metric": "gauge"
                                },
                                "executed_searches_rate": {
                                  "type": "long",
                                  "time_series_metric": "gauge"
                                },
                                "executed_searches_last_period": {
                                  "type": "long",
                                  "time_series_metric": "gauge"
                                },
                                "queue_size": {
                                  "type": "long",
                                  "time_series_metric": "gauge"
                                },
                                "remote_requests_current": {
                                  "type": "long",
                                  "time_series_metric": "gauge"
                                },
                                "remote_requests_total": {
                                  "type": "long",
                                  "time_series_metric": "gauge"
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                },
                "name": {
                  "type": "keyword",
                  "time_series_dimension": true
                },
                "id": {
                  "type": "keyword",
                  "time_series_dimension": true
                }
              }
            }
          }
        },
        "sample_date": {
          "type": "date"
        },
        "@version": {
          "type": "integer"
        },
        "metricset": {
          "properties": {
            "name": {
              "type": "keyword",
              "time_series_dimension": true
            }
          }
        },
        "event": {
          "properties": {
            "dataset": {
              "type": "keyword",
              "time_series_dimension": true
            },
            "module": {
              "type": "keyword",
              "time_series_dimension": true
            }
          }
        },
        "tags": {
          "type": "keyword",
          "time_series_dimension": true
        }
      }
    }
  },
  "_meta": {
    "description": "The default mappings for the Elasticsearch ingest pipelines monitoring"
  }
}