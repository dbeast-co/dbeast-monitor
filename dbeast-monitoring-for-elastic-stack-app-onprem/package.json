  "description": "DBeast Monitoring toolkit for Elastic Stack",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production ",
    "mage": "mage -v",
    "mage_win": "c:\\app\\mage_1.15.0_Windows-64bit\\mage -v",
