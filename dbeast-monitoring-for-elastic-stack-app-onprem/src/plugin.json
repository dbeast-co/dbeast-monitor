    {
      "type": "page",
      "icon": "cog",
      "name": "Configuration",
      "path": "/plugins/%PLUGIN_ID%",
      "role": "Admin",
      "addToNav": false
    },
    {
      "type": "page",
      "name": "Cluster's list",
      "path": "/a/dbeast-dbeastmonitor-app",
      "role": "Viewer",
      "uid": "clusters-list",
      "addToNav": true,
